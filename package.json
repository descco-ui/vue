{
  "name": "@descco/ui-vue",
  "version": "0.1.0",
  "description": "Descco UI Vue",
  "main": "lib/index.js",
  "scripts": {
    "build": "npm run build:js",
    "build:js": "cross-env NODE_ENV=production && babel source --out-dir lib && webpack --progress --config ./internals/webpack/webpack.config.js",
    "build-storybook": "cross-env NODE_ENV=development build-storybook -c ./internals/storybook -o .out",
    "lint": "npm run lint:js",
    "lint:js": "eslint source/**",
    "precommit": "npm run lint",
    "prepush": "npm test",
    "test": "jest --setupTestFrameworkScriptFile=./internals/test/setup.js --testRegex=/source/.*.test.js",
    "test:tdd": "npm test -- --watch",
    "test:coverage": "npm test -- --coverage --coverageDirectory=coverage",
    "storybook": "start-storybook -p 9000 -c ./internals/storybook"
  },
  "engines": {
    "node": ">=6.10.3",
    "npm": ">=3.10.10"
  },
  "directories": {
    "doc": "docs"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/descco-ui/vue.git"
  },
  "keywords": [
    "descco",
    "ui-vue"
  ],
  "author": "Hemerson Vianna <hemerson.lourenco@gmail.com> (https://github.com/hemersonvianna)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/descco-ui/vue/issues"
  },
  "homepage": "https://github.com/descco-ui/vue#readme",
  "devDependencies": {
    "@storybook/addon-events": "3.2.6",
    "@storybook/addon-info": "3.2.9",
    "@storybook/addon-knobs": "3.2.8",
    "@storybook/addon-notes": "3.2.7",
    "@storybook/addon-options": "3.2.6",
    "@storybook/vue": "3.2.8",
    "babel-cli": "6.24.1",
    "babel-eslint": "7.2.3",
    "babel-jest": "20.0.3",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-stage-0": "6.24.1",
    "babel-preset-vue": "1.0.2",
    "babel-register": "6.24.1",
    "chai": "4.1.1",
    "eslint": "4.4.0",
    "eslint-plugin-vue": "3.11.0",
    "html-webpack-plugin": "2.30.1",
    "husky": "0.14.3",
    "jest": "20.0.4",
    "jsdom": "11.1.0",
    "vue-eslint-parser": "1.0.0"
  },
  "dependencies": {
    "@descco/ui-core": "0.2.0",
    "babel-loader": "7.1.1",
    "css-loader": "0.28.4",
    "eslint-loader": "1.9.0",
    "extract-text-webpack-plugin": "3.0.0",
    "file-loader": "0.11.2",
    "node-sass": "4.5.3",
    "postcss-cssnext": "3.0.2",
    "postcss-loader": "2.0.6",
    "precss": "2.0.0",
    "sass-loader": "6.0.6",
    "style-loader": "0.18.2",
    "vue": "2.4.2",
    "vuex": "2.3.1",
    "webpack": "3.5.2",
    "yargs": "8.0.2"
  }
}
